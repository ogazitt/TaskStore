        <tag> out = rules.latest(); </tag>
      </item>
      <!-- 10 thru 99 -->
      <item weight="0.9">
        <ruleref uri="#NATURALNUMBERS_10_THRU_99"/>
        <tag> out = rules.latest(); </tag>
      </item>
    </one-of>
  </rule>

  <rule id="NATURALNUMBERS_10_THRU_99" scope="public">
    <one-of>
      <!-- 10 -->
      <item>
        ten
        <tag>out = 10;</tag>
      </item>
      <!-- decade -->
      <item>
        <ruleref uri="#SUB_DECADE"/>
        <tag>out = rules.latest();</tag>
      </item>
      <!-- 11 thru 99 no trailing zero -->
      <item>
        <ruleref uri="#NATURALNUMBERS_11_THRU_99"/>
        <tag> out = rules.latest() </tag>
      </item>
    </one-of>
  </rule>
 
